{
  "name": "xequtive-backend",
  "version": "1.0.0",
  "description": "Backend for Xequtive taxi booking service",
  "main": "dist/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
