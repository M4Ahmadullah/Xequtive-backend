    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "firebase-admin": "^12.7.0",
    "helmet": "^7.2.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "resend": "^4.5.2",
    "ts-node": "^10.9.2",
    "winston": "^3.11.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.12",
